{
  "compilerOptions": {
    "target": "es5",
    "module": "es2015",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [
      "es2015",
      "dom",
      "es2016"
    ],
    "noImplicitAny": true,
    "suppressImplicitAnyIndexErrors": true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "baseUrl": "",
    "paths": {
      "root/*": [
        "*"
      ],
      "common/*": [
        "src/common/*"
      ],
      "app/*": [
        "src/modules/app/*"
      ]
    },
    "outDir": "./compiled/src"
  },
  "files": [
    "./src/main.ts",
    "./src/modules/app/index.ts",
    "./src/modules/diseaseCenter/index.ts",
    "./src/modules/doctorCenter/index.ts",
    "./src/typings/json.d.ts"
  ],
  "exclude": [
    "node_modules"
  ],
  "angularCompilerOptions": {
    "genDir": "./compiled/aot",
    "skipMetadataEmit": true
  }
}
